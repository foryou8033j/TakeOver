<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mil.navy.takingover.view.tab.tippack.TipLayoutController">
   <center>
      <ListView fx:id="listView" BorderPane.alignment="CENTER">
         <tooltip>
            <Tooltip text="오른쪽 마우스를 눌러 데이터를 추가하세요." />
         </tooltip></ListView>
   </center>
   <bottom>
      <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="5.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="textField" prefHeight="23.0" prefWidth="205.0" />
            <Button fx:id="search" mnemonicParsing="false" onAction="#handleSearch" prefHeight="23.0" prefWidth="61.0" text="검색" />
         </children>
      </FlowPane>
   </bottom>
</BorderPane>
