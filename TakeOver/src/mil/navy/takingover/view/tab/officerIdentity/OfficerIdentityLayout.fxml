<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mil.navy.takingover.view.tab.officerIdentity.OfficerIdentityLayoutController">
   <children>
      <TableView fx:id="table" editable="true" layoutX="14.0" layoutY="14.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="32.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <tooltip>
            <Tooltip text="오른쪽 마우스를 눌러 데이터를 추가하세요." />
         </tooltip>
      </TableView>
      <ButtonBar layoutX="50.0" layoutY="566.0" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="15.0">
        <buttons>
          <Button fx:id="save" mnemonicParsing="false" onAction="#handleSave" text="저장" />
            <Button fx:id="load" mnemonicParsing="false" onAction="#handleLoad" text="불러오기" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
