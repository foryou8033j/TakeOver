<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="150.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mil.navy.takingover.view.InternetUserLayoutController">
   <children>
      <GridPane alignment="CENTER" layoutX="14.0" layoutY="70.0" AnchorPane.bottomAnchor="70.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0">
        <columnConstraints>
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="분류" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="사용자" GridPane.columnIndex="1" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="IP" GridPane.columnIndex="2" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="등록정책" GridPane.columnIndex="3" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="등록근거" GridPane.columnIndex="4" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="작업자" GridPane.columnIndex="5" />
            <ComboBox fx:id="object" prefWidth="150.0" GridPane.rowIndex="1" />
            <TextField fx:id="name" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="ip" GridPane.columnIndex="2" GridPane.rowIndex="1" />
            <ComboBox fx:id="policy" prefWidth="150.0" GridPane.columnIndex="3" GridPane.rowIndex="1" />
            <TextField fx:id="ground" GridPane.columnIndex="4" GridPane.rowIndex="1" />
            <TextField fx:id="worker" GridPane.columnIndex="5" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <ButtonBar layoutX="224.0" layoutY="146.0" prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="5.0">
        <buttons>
          <Button fx:id="accept" mnemonicParsing="false" onAction="#handleAccept" text="확인" />
            <Button fx:id="cancle" mnemonicParsing="false" onAction="#handleCancle" text="취소" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
